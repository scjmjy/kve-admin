{
    "compilerOptions": {
        "target": "esnext",
        "useDefineForClassFields": true,
        "module": "esnext",
        "moduleResolution": "node",
        "strict": true,
        "skipLibCheck": true,
        "jsx": "preserve",
        "allowJs": true,
        "sourceMap": true,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "lib": ["esnext", "dom"],
        "types": [
            "../../node_modules/admin-common/global",
            "../../node_modules/vite/client",
            "../../node_modules/element-plus/global"
        ],
        "baseUrl": "./",
        "paths": {
            "@/*": ["src/*"]
        }
    },
    "include": ["types/**/*.d.ts", "src/**/*.ts", "src/**/*.tsx", "src/**/*.vue", "build/**/*.ts", "vite.config.ts"]
    // "references": [{ "path": "./tsconfig.node.json" }]
}
